#um codigo que retornara um número inteiro dividido por 10.
while True:
	try:
		numero = int(input("Digite um número inteiro para dividir 10:"))
		if numero == 0:
			print("Erro: não é possivel dividir por zero.")
		else:
			resultado == 10 / numero
			primt(f"Resultado da divisão: 10 / {númeto} = {resultado}")
			break
	except Value Error:
		print("Errro!!!! o valor digitado não é número inteiro válido!!!")		
